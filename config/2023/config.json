        {
          "code": "tf",
          "title": "Lower Cone Scored",
          "icon": "cone",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "to",
          "title": "Upper Cube Scored",
          "icon": "cube",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tl",
          "title": "Middle Cube Scored",
          "icon": "cube",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tn",
          "title": "Lower Cube Scored",
          "icon": "cube",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
