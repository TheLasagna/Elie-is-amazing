        {
          "code": "dr",
          "title": "Defense Rating",
          "type": "select",
          "choices": {
            "1": "⭐️",
            "2": "⭐️⭐️",
            "3": "⭐️⭐️⭐️",
            "4": "⭐️⭐️⭐️⭐️",
            "5": "⭐️⭐️⭐️⭐️⭐️",
            "x": "Not Observed"
