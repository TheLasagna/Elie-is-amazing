          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "cl",
          "title": "Charge Station",
          "type": "select",
          "choices": {
            "DOCK": "Docked",
            "DOCK ENGAGE": "Docked and Engaged",
            "PARKED": "Parked",
            "X": "Not Attempted"
          },
          "defaultValue": "X",
          "required": false
        },
        {
          "code": "cn",
          "title": "# of alliance bots docked",
          "type": "counter",
          "defaultValue": 0,
          "required": false
        },
        {
          "code": "time",
          "title": "Time taken to dock",
          "type": "range",
          "min": 0,
          "max": 30,
          "defaultValue": 0,
          "required": false
        }
      ]
    },
    {
