        {
          "code": "alcm",
          "title": "Lower Cube Scored",
          "icon": "cube",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "ad",
          "title": "Docked",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "ae",
          "title": "Engaged",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "tu",
          "title": "Upper Cone Scored",
          "icon": "cone",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
