{
  "$schema": "../schema.json",
  "version": 1.2,
  "title": "QRScout",
  "page_title": "Cressendo",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true,
      "fields": [
        {
          "title": "Scouter First Name",
          "type": "text",
          "required": true,
          "code": "scouter"
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber"
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "choices": {
            "R1": "Red Top",
            "R2": "Red Middle",
            "R3": "Red Bottom",
            "B1": "Blue Top",
            "B2": "Blue Middle",
            "B3": "Blue Bottom"
          },
          "defaultValue": "R1"
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber"
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "noShow"
        },
        {
          "title": "Using Human Player",
          "type": "boolean",
          "required": false,
          "defaultValue": false,
          "code": "humanPlayer"
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Starting Location",
          "type": "select",
          "choices": {
            "left": "Top",
            "middle": "Middle",
            "right": "Bottom"
          },
          "defaultValue": "left",
          "required": false,
          "code": "autoStart"
        },
        {
          "code": "am",
          "title": "Working Auton?",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "auc",
          "title": "Upper Cone Scored",
          "icon": "cone",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "aucm",
          "title": "Middle Cone Scored",
          "icon": "cone",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "amc",
          "title": "Lower Cone Scored",
          "icon": "cone",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "amcm",
          "title": "Upper Cube Scored",
          "icon": "cube",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
